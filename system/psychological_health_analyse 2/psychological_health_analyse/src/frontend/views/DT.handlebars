<script>

function delete_airline_by_ajax(obj){
      let item = ""
      for(let i of obj){
          item += $(i).text()
          //console.log('111');
          //console.log($(i).text());
      }
      item = item.replace(/\n/g,"##").replace(/\s/g,"").replace(/##$/,"").replace(/^##/,"")
      console.log(item);
      $.ajax({
        url:"/delete_airline_api",
        type:"POST",
        data:{item},
        success:(data) =>{
            if(data === "ok")
                obj.css("display","none")
            else {
                alert("delete failed")
            }
        }
      })
}

function abc(obj){
  $.confirm({
      title: 'Confirm!',
      content: 'Do you confirm to delete the airline?',
      buttons: {

          confirm: {
              text: 'confirm',
              btnClass: 'btn-blue',
              keys: ['enter', 'shift'],
              action: function(){
                  delete_airline_by_ajax(obj)
              }
          },
          cancel: {
              text: 'cancel',
              btnClass: 'btn-blue',
              keys: ['enter', 'shift'],
              action: function(){

              }
          }
      }
  });
}

</script>
